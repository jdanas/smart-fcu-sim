{
  "name": "smart-fcu-sim",
  "version": "1.0.0",
  "description": "Non-intrusive Plug-and-Play Adaptive HVAC Control System",
  "scripts": {
    "dev": "docker compose up --build",
    "dev:backend": "docker compose up --build backend",
    "dev:frontend": "docker compose up --build frontend",
    "prod": "docker compose --env-file .env.production up --build",
    "down": "docker compose down",
    "clean": "docker compose down -v --rmi local",
    "logs": "docker compose logs -f",
    "logs:backend": "docker compose logs -f backend",
    "logs:frontend": "docker compose logs -f frontend"
  },
  "keywords": ["hvac", "iot", "dashboard", "fastapi", "react"],
  "license": "MIT"
}
